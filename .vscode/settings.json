{
    "files.autoSave": "onWindowChange",
    "editor.tabSize": 2,
    "eslint.validate": [
        "typescript",
        "javascript"
    ],
    "eslint.format.enable": true,
    "files.eol": "\n",

    "[typescript]" : {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },

    "[javascript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    
}